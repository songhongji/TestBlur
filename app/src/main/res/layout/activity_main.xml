<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.winter.testblur.MainActivity">

    <Button
        android:id="@+id/test_render_script_blur"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test Render Script" />

    <Button
        android:id="@+id/test_java_blur"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test Java Blur" />

    <Button
        android:id="@+id/test_advanced_java_blur"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test Advanced Java Blur" />

    <Button
        android:id="@+id/test_jni_blur"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test JNI Blur" />

    <Button
        android:id="@+id/test_all_blur"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Test All Blur" />
</LinearLayout>
